datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model CurrencyRate {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  from CurrencyCode
  to CurrencyCode
  result Float
  date String

  @@map("currencyRates")
}

enum CurrencyCode {
  AZN
  GBP
  USD
  UAH
  RUB
  TRY
  EUR
}
